[alias]
	amend = "commit --amend"
	cloner = "clone --recursive"
	continue = "rebase --continue"
	fixup = "commit --amend -C HEAD"
	head = "log -1 HEAD"
	pop = "stash pop"
	pure = "pull --rebase"
	review = "diff --word-diff=color --staged"
	st = "status -sb"
	unstage = "reset HEAD --"
